	'atlas_hub_database' as sourceserver
	,'atlas_hub' as SourceDB
	,'atlas_hub_project' as SourceTable
	,name
	,description
	,null as detaileddescription
	,'atlas-hub-project' as reportobjecttype
	,CONCAT('YOURORGNAME\',u.account_name) as author
	,CONCAT('YOURORGNAME\',u2.account_name) as lastmodifiedby
	,cast(updated as date) as lastmodifieddate
	,CONCAT('https://taskscheduler.example.com/project/',p.id) as ReportObjectURL
	,null as epicmasterfile
	,p.id as epicrecordid
	,null as reportservercatalogid
	,'Y' as DefaultVisibilityYN
	,NULL AS ReportTemplateId
	,'Public' as Availability

	
from project p
left join public."user" u on u.id=p.owner_id
left join public."user" u2 on u2.id=p.updater_id</property>
