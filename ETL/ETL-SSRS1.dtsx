BSTRING(ExtensionSettings, CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;',ExtensionSettings), 10000), '&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ''))-1&#xA;    ) AS EmailList&#xA;    ,[Description]&#xA;    ,[LastStatus]&#xA;    ,[LastRunTime]&#xA;    ,REPLACE(LEFT(&#xA;      REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;',ExtensionSettings), 10000), '&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ''), &#xA;      CHARINDEX('&lt;/Value&gt;', REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;',ExtensionSettings), 10000), '&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ''))-1&#xA;      ), '&lt;ParameterValues&gt;&lt;ParameterValue&gt;&lt;Name&gt;PATH&lt;/Name&gt;&lt;Value&gt;', '')&#xA;&#xA; FROM [raw].[ssrs_database-reportserver-dbo-subscriptions] Subscriptions&#xA;&#xA; WHERE Subscriptions.ExtensionSettings NOT LIKE '%;%' &#xA;   and LEFT(&#xA;     REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;',ExtensionSettings), 10000), '&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ''), &#xA;     CHARINDEX('&lt;/Value&gt;', REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;',ExtensionSettings), 10000), '&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ''))-1&#xA;    )  not like '&lt;Parameter%')&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
