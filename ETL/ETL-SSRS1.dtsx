                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">(SELECT distinct [SubscriptionID]
    ,[Report_OID]
    ,[InactiveFlags]
    ,LEFT(
     REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;',ExtensionSettings), 10000), '&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ''), 
     CHARINDEX('&lt;/Value&gt;', REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;',ExtensionSettings), 10000), '&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ''))-1
    ) AS EmailList
    ,[Description]
    ,[LastStatus]
    ,[LastRunTime]
    ,LTRIM(RTRIM(s.Value)) SubscriptionTo
 FROM [raw].[ssrs_database-reportserver-dbo-subscriptions] Subscriptions
   CROSS APPLY STRING_SPLIT(
        LEFT(
          REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;',ExtensionSettings), 10000), '&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ''), 
          CHARINDEX('&lt;/Value&gt;', REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;',ExtensionSettings), 10000), '&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ''))-1
         ), ';') as s

 WHERE Subscriptions.ExtensionSettings LIKE '%;%' AND s.value IS NOT NULL and s.value != '') 

