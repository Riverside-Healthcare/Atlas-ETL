                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">with #SSRS_XMLOne as ( 
select 
  ItemId 
, CONVERT(xml, 
   CASE 
    WHEN LEFT(Content,3) = 0xEFBBBF  THEN CONVERT(varbinary(max),SUBSTRING(Content,4,LEN(Content))) 
    ELSE Content 
   END,1) AS Content 
  
from 
( 
  SELECT 
  ItemID 
  ,CONVERT(varbinary(max),Content) AS Content 
 FROM raw.[ssrs_database-reportserver-dbo-catalog] 
WHERE Type IN (2,5,7,8) 
) as t 
  
 ) 
 --- query two 
select 
concat( 
       'ssrs_database' 
    ,   '||' 
    ,   'reportserver' 
    ,   '||' 
    ,     case 
                    when r.Type = 1 then N'SSRS Folder' 
                    when r.Type = 2 then N'SSRS Report' 
                    when r.Type = 3 then N'SSRS File' 
                    when r.Type = 4 then N'SSRS Linked Report' 
                    when r.Type = 5 then N'SSRS Datasource' 
                    when r.Type = 6 then N'SSRS Model' 
                    when r.Type = 8 then N'SSRS Shared Dataset' 
                    when r.Type = 9 then N'SSRS Report Part' 
                    when r.Type = 11 then N'SSRS KPI' 
                    when r.Type = 12 then N'SSRS Mobile Report (folder)' 
                    when r.Type = 13 then N'SSRS Power BI Desktop Document' 
                    else N'SSRS Unknown' 
                end      
    ,   '||' 
    ,   r.ItemId 
    ,   '||||'  
   
    ) as BizKey 
,t.query as Query 
,getdate() as LastLoadDate 
From ( 
select * from ( 
SELECT 
    ItemID 
    ,T.Query.value('(./*:CommandText/text())[1]','nvarchar(max)') AS Query 
      
FROM #SSRS_XMLOne 
--Get all the Query elements (The "*:" ignores any xml namespaces) 
CROSS APPLY #SSRS_XMLOne.Content.nodes('//*:Query') T(Query) 
  
)  as t 
where Query is not null) as t 
inner join raw.[ssrs_database-reportserver-dbo-catalog] r on r.ItemId=t.ItemID</property>
