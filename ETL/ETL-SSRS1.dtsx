              name="ssrs_database SSRS"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select          N'ssrs_database'                                                                                            as SourceServer
,               N'reportserver'                                                                                        as SourceDB
,               N'catalog'                                                                                             as SourceTable
,               c.Name
,               c.Description
,               null                                                                                                   as DetailedDescription
,               case
                    when c.Type = 1 then N'SSRS Folder'
                    when c.Type = 2 then N'SSRS Report'
                    when c.Type = 3 then N'SSRS File'
                    when c.Type = 4 then N'SSRS Linked Report'
                    when c.Type = 5 then N'SSRS Datasource'
                    when c.Type = 6 then N'SSRS Model'
                    when c.Type = 8 then N'SSRS Shared Dataset'
                    when c.Type = 9 then N'SSRS Report Part'
                    when c.Type = 11 then N'SSRS KPI'
                    when c.Type = 12 then N'SSRS Mobile Report (folder)'
                    when c.Type = 13 then N'SSRS Power BI Desktop Document'
                    else N'SSRS Unknown'
                end                                                                                                    as ReportObjectType
,               author.UserName                                                                                        as Author
,               modder.UserName                                                                                        as LastModifiedBy
,               c.ModifiedDate                                                                                         as LastModifiedDate
--,               concat(N'https://ssrs_database.example.com/Reports/manage/catalogitem/properties', c.Path)      as ReportObjectURL
,       CASE WHEN c.[Type] in (2,4) --report, linked report
            THEN N'https://ssrs_database.example.com/ReportServer/Pages/ReportViewer.aspx?' + REPLACE(REPLACE(c.[Path], '/', '%2f'), ' ', '+') + N'&amp;rs:Command=Render'
          WHEN c.[Type]=1 --folder
            THEN N'https://ssrs_database.example.com/Reports/browse' + c.[Path]
        ELSE concat(N'https://ssrs_database.example.com/Reports/manage/catalogitem/properties', c.Path)
        END                                                    as ReportObjectURL
,               null                                                                                                     as EpicMasterFile
,               null                                                                                                     as EpicRecordID
--,               concat(N'https://ssrs_database.example.com/Reports/manage/catalogitem/properties', parent.Path) as ParentURL
,       CASE WHEN parent.[Type] in (2,4) --report, linked report
            THEN N'https://ssrs_database.example.com/ReportServer/Pages/ReportViewer.aspx?' + REPLACE(REPLACE(parent.[Path], '/', '%2f'), ' ', '+') + N'&amp;rs:Command=Render'
          WHEN parent.[Type]=1 --folder
            THEN N'https://ssrs_database.example.com/Reports/browse' + parent.[Path]
        ELSE concat(N'https://ssrs_database.example.com/Reports/manage/catalogitem/properties', parent.Path)
        END                                                    as ParentURL
,               null                                                                                                     as ParentMasterFile
,               null                                                                                                     as ParentRecordID
,       concat(N'', c.ItemID)                                                as ReportServerCatalogID
,       case 
          when hbiuser_access.ItemID is not null 
              and c.Type in (2,4) --report, linked report
            then N'Y'
          else N'N'
        end                                                    as DefaultVisibilityYN 
,       NULL                                                   as EpicReportTemplateId
,       concat(N'', c.Path)                                            as ReportServerPath

from            [raw].[ssrs_database-reportserver-dbo-catalog] as c
